{
  "name": "personal-website",
  "private": true,
  "description": "This is my personal site powered by isomorphic react app.",
  "version": "1.0.0",
  "engines": {
    "node": ">=6",
    "npm": ">=5.3"
  },
  "homepage": "http://github.com/mitya-borodin/personal-website",
  "repository": {
    "type": "git",
    "url": "git@github.com:mitya-borodin/personal-website.git"
  },
  "bugs": {
    "url": "https://github.com/mitya-borodin/personal-website/issues"
  },
  "author": {
    "name": "Dmitriy Borodin",
    "email": "dmitriy@borodin.site",
    "url": "http://borodin.site"
  },
  "scripts": {
    "start": "react-app --onlyServer -i",
    "start:debug": "react-app --onlyServer -i -d",
    "start:analyze": "react-app --onlyServer -i -a -d",
    "start:release": "react-app --onlyServer -i -r",
    "build": "react-app build --onlyServer -i -d -a",
    "release": "react-app build --onlyServer -i -r",
    "release:analyze": "react-app build --onlyServer -i -r -a -d",
    "postinstall": "npm i -g rearguard@^1"
  },
  "dependencies": {
    "express": "4.15.4",
    "isomorphic-style-loader": "3.0.0",
    "normalize.css": "7.0.0",
    "pretty-error": "2.1.1",
    "prop-types": "15.5.10",
    "react": "15.6.1",
    "react-dom": "15.6.1"
  },
  "devDependencies": {
    "eslint-config-techintouch": "1.5.0",
    "postcss-extends": "1.0.0"
  }
}
