{
  "name": "personal-website",
  "private": true,
  "description": "This is my personal site powered by isomorphic react app.",
  "version": "1.0.0",
  "engines": {
    "node": ">=8",
    "npm": ">=5.3"
  },
  "homepage": "http://github.com/mitya-borodin/personal-website",
  "repository": {
    "type": "git",
    "url": "git@github.com:mitya-borodin/personal-website.git"
  },
  "bugs": {
    "url": "https://github.com/mitya-borodin/personal-website/issues"
  },
  "author": {
    "name": "Dmitriy Borodin",
    "email": "dmitriy@borodin.site",
    "url": "http://borodin.site"
  },
  "scripts": {
    "start": "rearguard infernojs start -i",
    "start:debug": "rearguard infernojs start -i -d",
    "start:analyze": "rearguard infernojs start -i -a -d",
    "start:release": "rearguard infernojs start -i -r",
    "build": "rearguard infernojs build -i -d -a",
    "release": "rearguard infernojs build  -i -r",
    "release:analyze": "rearguard infernojs build -i -r -a -d"
  },
  "dependencies": {
    "express": "4.15.4",
    "hoist-non-inferno-statics": "1.1.3",
    "inferno": "3.7.1",
    "inferno-component": "3.7.1",
    "inferno-server": "3.7.1",
    "normalize.css": "7.0.0",
    "pretty-error": "2.1.1",
    "prop-types": "15.5.10"
  }
}
