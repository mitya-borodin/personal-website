{
  "private": true,
  "name": "personal-website",
  "description": "This is my personal site powered by isomorphic react app.",
  "author": "Dmitriy Borodin <dmitriy@borodin.site> (http://borodin.site)",
  "homepage": "http://github.com/mitya-borodin/personal-website",
  "repository": {
    "type": "git",
    "url": "git@github.com:mitya-borodin/personal-website.git"
  },
  "bugs": {
    "url": "https://github.com/mitya-borodin/personal-website/issues"
  },
  "version": "1.0.0",
  "files": [
    "dll_bundle"
  ],
  "scripts": {
    "build": "rearguard build --application",
    "build:both": "rearguard build --both --application",
    "build:release": "rearguard build -r --application",
    "check_deps_on_npm": "rearguard check_deps_on_npm",
    "check_deps_on_npm:install": "rearguard check_deps_on_npm --install_deps",
    "dll": "rearguard build --dll",
    "dll:release": "rearguard build --dll -r",
    "prepublishOnly": "npm run build:both",
    "start": "rearguard wds",
    "start:release": "rearguard wds -r"
  },
  "dependencies": {
    "isomorphic-style-loader": "5.0.1",
    "normalize.css": "8.0.1",
    "pretty-error": "2.1.1",
    "react": "16.8.3",
    "react-dom": "16.8.3",
    "tslib": "1.9.3"
  },
  "devDependencies": {
    "@types/node": "11.10.4",
    "@types/prop-types": "15.7.0",
    "@types/react": "16.8.6",
    "@types/react-dom": "16.8.2",
    "@types/webpack": "4.4.25",
    "postcss-css-variables": "0.12.0",
    "tslint": "5.13.1",
    "tslint-no-circular-imports": "0.6.1",
    "typescript": "3.3.3"
  },
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "rearguard": {
    "context": "src",
    "entry": "index.tsx",
    "dll_entry": "vendors.ts",
    "modules": [
      "src"
    ],
    "output": {
      "path": "dist",
      "publicPath": "/"
    },
    "sync_project_deps": [],
    "has_dll": true,
    "has_browser_lib": false,
    "load_on_demand": false,
    "is_application": true,
    "has_node_lib": false,
    "publish_in_git": false
  }
}