{
  "context": "src",
  "entry": "index.tsx",
  "output": {
    "path": "dist",
    "publicPath": "/"
  },
  "modules": ["src"],
  "npmHardSync": [],
  "typescript": {
    "configPath": "tsconfig.json",
    "config": {
      "compilerOptions": {
        "importHelpers": true,
        "noImplicitAny": false,
        "noUnusedParameters": false,
        "strictPropertyInitialization": false,
        "types": ["node"],
        "paths": []
      },
      "compileOnSave": false
    }
  },
  "postCSS": {
    "plugins": "postCssPlugins.js"
  }
}
